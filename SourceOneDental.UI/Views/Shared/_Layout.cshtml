<body>

    <div id="loading" class="se-pre-con">
        <p class="loadtext">
            <img src="/Content/images/logo.jpg" /><br />Please, Wait...
        </p>
    </div>

    <div class="modal-shiftfix">
        <!-- Navigation -->
        <div class="navbar navbar-fixed-top scroll-hide">
            <div class="container-fluid top-bar">
                <div class="pull-right">
                    <ul class="nav navbar-nav pull-right">
                        <li class="dropdown user hidden-xs">
                            <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                                <img width="34" height="34" src="/Content/images/avatar-male.jpg" />@ViewBag.DisplayName<b class="caret"></b>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <a href="#">
                                        <i class="icon-user"></i>My Account
                                    </a>
                                </li>
                                <li>
                                    <a href="#">
                                        <i class="icon-gear"></i>Account Settings
                                    </a>
                                </li>
                                <li>
                                    <a href="javascript:document.getElementById('logout-form').submit();" onclick="return confirm('Está seguro de que desea salir?');">
                                        <i class="icon-signout"></i>Logout
                                    </a>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <a href="/Home"><img src="/Content/images/logo.jpg" height="40" /></a>
            </div>
            @Html.Action("_Menu", "Home")
        </div>
        <!-- End Navigation -->
        <div class="container-fluid main-content">
            <div class="row">
               @RenderBody()
            </div>
        </div>
    </div>
    <form id="logout-form" action="/Account/SignOut" method="POST"></form>
    <script>
        $('#loading').hide();
    </script>
