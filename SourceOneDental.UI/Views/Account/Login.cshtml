<body class="login1">

    <div id="loading" class="se-pre-con">
        <p class="loadtext">
            <img src="/Content/images/logo.jpg" /><br />Please, Wait...
        </p>
    </div>

    <div id="msgPanel" class="alert alert-danger" style="display: none">
        <button class="close" data-dismiss="alert" type="button">&times;</button><span id="msgText"></span>
    </div>

    <!-- Login Screen -->
    <div class="login-wrapper">
        <div class="login-container">
            <a href="http://sourceonedental.com"><img src="/Content/images/logo.jpg" /></a>
                <div class="form-group">
                    <input id="username" class="form-control" placeholder="Username or Email" type="text">
                </div>
                <div class="form-group">
                    <input id="password" class="form-control" placeholder="Password" type="password"><input id="login-button" type="button" value="&#xf054;">
                </div>
                <div class="form-options clearfix">
                    <div class="text-left">
                        <label class="checkbox"><input type="checkbox"><span>Remember me</span></label>
                    </div>
                </div>
            <p class="signup">
                ©2014 SourceOne Dental, Inc.
            </p>
        </div>
    </div>
    
    <script src="/JSBackend/utils.js"></script>

    <script>
        $(function () {
            $('#loading').hide();
            $('#username').focus();
        });

        $('.enter').keypress(function (event) {
            if (event.which == 13) {
                event.preventDefault();
                login();
            }
        });
    </script>

    <script src="/JSBackend/account/login.js"></script>

